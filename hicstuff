pip3 install -U hicstuff
hicstuff pipeline --help
export PATH=/home/em_b/bowtie2/bowtie2-2.5.3-sra-linux-x86_64:$PATH

hicstuff pipeline --aligner bowtie2 --enzyme "DpnII,HinfI" --binning 1000 
--duplicates --filter --distance-law --plot --threads 20 
--genome /home/em_b/bowtie2/GRCh38_index/index 
SRR17223321_filtered_R1.fastq.gz SRR17223321_filtered_R2.fastq.gz
